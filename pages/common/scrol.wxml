<template name="scrol">
    <view class="scrolling">
        <swiper class="scrolling-s" indicator-dots="true" autoplay="true" duration="{{ duration }}" circular="true" bindchange="">
            <block wx:for="{{ scrollist }}" wx:key="">
              <swiper-item>
                  <image src="{{ item.image }}" class="img" />
              </swiper-item>
            </block>
        </swiper>
    </view>
</template>